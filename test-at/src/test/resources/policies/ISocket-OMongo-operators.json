{
  "name": "ATSocketMongo",
  "duration": 2000,
  "saveRawData": "false",
  "rawDataParquetPath": "myTestParquetPath",
  "checkpointDir": "checkpoint",
  "timePrecision": "minute",
  "checkpointGranularity": "minute",
  "checkpointInterval": 30000,
  "checkpointTimeAvailability": 60000,
  "inputs": [
    {
      "name": "in-socket",
      "elementType": "SocketInput",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ],
  "dimensions": [
    {
      "name": "product",
      "dimensionType": "PassthroughDimension"
    },
    {
      "name": "price",
      "dimensionType": "PassthroughDimension"
    }
  ],
  "rollups": [
    {
      "dimensionAndPrecision": [
        {
          "dimensionName": "product",
          "precision": "identity"
        }
      ],
      "operators": ["accum-operator","avg-operator", "count-operator", "first-value-operator", "full-text-operator",
        "last-value-operator", "max-operator", "median-operator", "min-operator", "stddev-operator", "sum-operator",
        "variance-operator", "range-operator"]
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "elementType": "MongoDbOutput",
      "configuration": {
        "clientUri": "mongodb://localhost:60000",
        "dbName": "csvtest",
        "multiplexer": "false"
      }
    }
  ],
  "operators": [
    {
      "name": "accum-operator",
      "elementType": "AccumulatorOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "avg-operator",
      "elementType": "AvgOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "count-operator",
      "elementType": "CountOperator",
      "configuration": {
      }
    },
    {
      "name": "first-value-operator",
      "elementType": "FirstValueOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "full-text-operator",
      "elementType": "FullTextOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "last-value-operator",
      "elementType": "LastValueOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "max-operator",
      "elementType": "MaxOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "median-operator",
      "elementType": "MedianOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "min-operator",
      "elementType": "MinOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "range-operator",
      "elementType": "RangeOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "stddev-operator",
      "elementType": "StddevOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "sum-operator",
      "elementType": "SumOperator",
      "configuration": {
        "inputField":"price"
      }
    },
    {
      "name": "variance-operator",
      "elementType": "VarianceOperator",
      "configuration": {
        "inputField":"price"
      }
    }
  ],
  "parsers": [
  {
    "name": "morphline-parser",
    "elementType": "MorphlinesParser",
    "configuration": {
      "morphline": {
        "id": "morphline1",
        "importCommands": [
          "org.kitesdk.**"
        ],
        "commands" : [
          {
            "readCSV" : {
              "separator" : ",",
              "columns" : ["product","price"],
              "trim" : "true",
              "charset" : "UTF-8",
              "quoteChar" : "\"",
              "commentPrefix" : "#"
            }
          }
        ]
      }
    }
  }
]
}